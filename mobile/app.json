{
  "expo": {
    "name": "WIMC",
    "slug": "wimc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wimc",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4285F4"
    },
    "android": {
      "package": "com.wimc.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4285F4"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAIy-E26PEXJaxeHVFf5VJTlMEI-HZMcpU"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.wimc.app",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAIy-E26PEXJaxeHVFf5VJTlMEI-HZMcpU"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow WIMC to use your location."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "b360a001-3258-4d41-90e0-394025b77890"
      }
    }
  }
}
